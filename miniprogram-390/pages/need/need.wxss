.title {
  font-size: 20px;
  color: #030303;
  margin-left: 10px;
}

.top_week {
  padding: 0 20px 0 20px;
  margin-top: 46px;
  height: 5vh;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.progress{
  width:400rpx;
  height: 400rpx;
  margin:300rpx auto;
  position: relative;
}
.progress .progress_bg,.progress .progress_active{
position: absolute;
left: 0;
top: 0;
width: 400rpx;
height: 400rpx;
}
.head-bg{  
  overflow: hidden;
  width: 750rpx;
  height: 1150rpx;
  background-image: url('https://636c-cloud1-7gy59557c11845e2-1307494875.tcb.qcloud.la/R-C.jpg?sign=70114034291aa1de44739533f7c15cd9&t=1655898108');
  background-size: 100% 100%;
}
.progress .progress_text{
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 35rpx;
  color:#ffffff;
  text-align: center;
  position: absolute;
  left:120rpx;
  top:170rpx;
}


.top_date {
  padding: 0 10px 0 10px;
  margin-top: 9px;
  height: 35px;
  display: flex;
  align-items: center;
  justify-content: space-between;

}
.btns .okbtn,.btns .pbtn,.btns .ctbtn,.btns .cabtn{
width: 280rpx;
height: 60rpx;
line-height: 60rpx;
text-align: center;
color:#ffffff;
border: 3rpx solid #ffffff;
border-radius: 20rpx;
margin:0 auto 20rpx auto;
}

.date_item {
  height: 35px;
  width: 35px;
  background-color: rgba(255, 255, 255, 100);
  color: rgba(16, 16, 16, 100);
  font-size: 14px;
  text-align: center;
  font-family: -apple-system;
  border: 1px solid rgba(237, 234, 244, 100);
  line-height: 35px;
  border-radius: 50%;


}

.week_item {
  color: rgba(102, 102, 102, 100);
  font-size: 12px;
  width: 12px;
  height: 17px;
}

.date_on {
  background-color: rgba(116, 104, 190, 100);
  color: rgba(255, 255, 255, 95);
}

.body {
  position: fixed;
  left: 20px;
  top: 190px;
  width: 300px;
  height: 402px;
  padding: 18px;
  line-height: 20px;
  overflow-y: auto;
  border-radius: 20px;
  background-color: rgba(255, 255, 255, 100);
  color: rgba(16, 16, 16, 100);
  font-size: 14px;
  text-align: center;
  box-shadow: 0px 2px 6px 0px rgba(178, 178, 191, 22);
  font-family: -apple-system;
  border: 1px solid rgba(237, 234, 244, 100);
}

.body_title {
  margin-bottom: 24px;
  height: 22px;
  color: rgba(3, 3, 3, 100);
  font-size: 16px;
  text-align: left;
}



.item_it {
  display: flex;
  align-items: center;
  height: 20px;
}

.it_left {
  width: 17px;
  height: 17px;
  border: 1px solid #7468BE;
  border-radius: 50%;
}

.it_image {
  width: 18px;
  height: 18px;
}

.it_date {
  width: 34px;
  height: 17px;
  color: rgba(116, 104, 190, 100);
  font-size: 14px;
  margin-left: 2vw;
}

.it_content {
  margin-left: 5vw;
  color: rgba(3, 3, 3, 74);
  font-size: 14px;
  font-family: PingFangSC-regular;
}

.border {
  height: 35px;
  border-left: 1px solid #7468BE;
  margin-left: 8.5px;
}

.it_content_right {
  margin-left: 8.5px;
  padding: 0.5vw 2vw;
  background-color: rgba(217, 247, 190, 100);
  color: rgba(56, 158, 13, 100);
  border-radius: 20px;
}

.bottom {
  position: fixed;
  left: 162px;
  bottom: 15px;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  line-height: 20px;
  text-align: center;
  background-color: rgba(116, 104, 190, 100);
  display: flex;
  align-items: center;justify-content: center;
}

.bottom image {
  width: 80%;
}